{
  "name": "metex",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "deploy": "npm run codegen && ng build --prod && npm run scully:ci",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --prod --protractor-config=e2e/protractor-ci.conf.js",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "scully": "scully",
    "scully:serve": "scully serve",
    "scully:ci": "scully -- --scanRoutes"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/localize": "~11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@apollo/client": "^3.2.9",
    "@fortawesome/angular-fontawesome": "^0.8.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@scullyio/init": "^1.0.1",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.5",
    "apollo-angular": "^2.1.0",
    "bootstrap": "^4.5.3",
    "dotenv": "^8.2.0",
    "factory.ts": "^0.5.1",
    "graphql": "^15.4.0",
    "ngx-markdown": "^10.1.1",
    "rxjs": "~6.6.0",
    "sass-color-helpers": "^2.1.1",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
