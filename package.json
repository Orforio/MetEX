{
  "name": "metex",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "deploy": "npm run codegen && ng build --configuration production && npm run scully:ci && npm run pwa:regenerate",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --configuration production --protractor-config=e2e/protractor-ci.conf.js",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "scully": "scully",
    "scully:serve": "scully serve",
    "scully:ci": "scully -- --scanRoutes",
    "icons": "npx pwa-asset-generator logo.html ./src/assets/icons -i ./src/index.html -m ./src/manifest.webmanifest -t png -i",
    "pwa": "npm run deploy && http-server -p 8080 -c-1 dist/static",
    "pwa:regenerate": "rm dist/static/ngsw.json;node_modules/.bin/ngsw-config dist/static ./ngsw-config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.13",
    "@angular/common": "~12.2.13",
    "@angular/compiler": "~12.2.13",
    "@angular/core": "~12.2.13",
    "@angular/forms": "~12.2.13",
    "@angular/localize": "~12.2.13",
    "@angular/platform-browser": "~12.2.13",
    "@angular/platform-browser-dynamic": "~12.2.13",
    "@angular/router": "~12.2.13",
    "@angular/service-worker": "~12.2.13",
    "@apollo/client": "^3.4.17",
    "@fortawesome/angular-fontawesome": "^0.10.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@scullyio/init": "^2.0.1",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.5",
    "apollo-angular": "^2.6.0",
    "bootstrap": "^4.5.3",
    "dotenv": "^8.2.0",
    "factory.ts": "^0.5.1",
    "graphql": "^15.7.2",
    "ngx-markdown": "^12.0.1",
    "rxjs": "~6.6.0",
    "sass-color-helpers": "^2.1.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~12.1.3",
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.13",
    "@gammastream/scully-plugin-http404": "^1.0.4",
    "@gammastream/scully-plugin-sitemap": "^1.0.4",
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-apollo-angular": "^2.2.1",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@scullyio/scully-plugin-flash-prevention": "^1.0.0",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.7",
    "codelyzer": "^6.0.1",
    "dotenv-webpack": "^5.1.0",
    "fast-xml-parser": "^3.17.4",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.8",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-mocks": "^12.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}